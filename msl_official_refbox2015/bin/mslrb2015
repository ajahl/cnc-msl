#!/bin/sh

APPDIR=$(dirname "$0")/../mslrb2015
cd $APPDIR
APPDIR=$PWD

java -Djna.nosys=true -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR/lib/json-20140107.jar:$APPDIR/lib/mslrb2015.jar:$APPDIR/lib/core.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all-natives-linux-amd64.jar:$APPDIR/lib/gluegen-rt-natives-linux-amd64.jar:$APPDIR/lib/net.jar" mslrb2015 "$@"

